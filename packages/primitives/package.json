{
  "name": "@strapi/primitives",
  "version": "1.6.1",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "develop": "vite build --watch",
    "prebuild": "rimraf dist",
    "build": "yarn build:prod && yarn generate:types",
    "build:prod": "vite build",
    "generate:types": "tsc --noEmit false --emitDeclarationOnly --declarationDir dist",
    "test": "jest -c jest.config.js",
    "test:ts": "tsc --noEmit",
    "test:watch": "jest -c jest.config.js --watchAll"
  },
  "dependencies": {
    "@radix-ui/primitive": "^1.0.0",
    "@radix-ui/react-collection": "^1.0.1",
    "@radix-ui/react-compose-refs": "^1.0.0",
    "@radix-ui/react-context": "^1.0.0",
    "@radix-ui/react-direction": "^1.0.0",
    "@radix-ui/react-dismissable-layer": "^1.0.2",
    "@radix-ui/react-focus-guards": "^1.0.0",
    "@radix-ui/react-focus-scope": "^1.0.1",
    "@radix-ui/react-id": "^1.0.0",
    "@radix-ui/react-popper": "^1.1.0",
    "@radix-ui/react-portal": "^1.0.1",
    "@radix-ui/react-primitive": "^1.0.1",
    "@radix-ui/react-slot": "^1.0.1",
    "@radix-ui/react-use-callback-ref": "^1.0.0",
    "@radix-ui/react-use-controllable-state": "^1.0.0",
    "aria-hidden": "^1.2.2",
    "react-remove-scroll": "^2.5.5"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
